                    "id": "#main/gvcf",
                    "outputSource": [
                        "#main/gatk_haplotypecallergvcf/gvcf"
                    ],
                    "type": [
                        "File",
                        {
                            "type": "array",
                            "items": "File"
                        }
                    ],
                }
            ],
            "steps": [
                {
                    "id": "#main/gatk_sortsam",
                    "in": [
                        {
                            "id": "#main/gatk_sortsam/input",
                            "source": "#main/gatk_mergebamalignment/output"
                        },
                        {
                            "id": "#main/gatk_sortsam/sort_order",
                            "default": "coordinate"
                        }
